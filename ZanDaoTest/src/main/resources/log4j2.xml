<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
	<Console name="Console" target="SYSTEM_OUT">
		<PatternLayout  pattern="%d{yyyy-MM-dd HH:mm:ss} [%t] %-5level  - %msg%n"/>
    </Console>
	<File name="log" fileName="./logs/test.log" append="false">
		<PatternLayout  charset="utf-8" pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level %class{36} - %msg%xEx%n"/>
	</File>
</Appenders>    
<Loggers>
	<Logger name="com.UIFrame.class2.Utils.Log" level="all">	    
    </Logger>
	<Root level="trace">
		<AppenderRef ref="Console"/>
		<AppenderRef ref="log"/>
    </Root>
  </Loggers>
</Configuration>